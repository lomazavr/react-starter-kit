{
  "name": "react-starter-kit",
  "version": "0.0.1",
  "author": "lomazavr",
  "license": "MIT",
  "description": "A light foundation for your next react frontend project based on webpack.",
  "scripts": {
    "lint": "yarn lint:styles && yarn lint:scripts && echo Linting complete successfully",
    "lint:styles": "stylelint src/**/*.{css,scss,sass}",
    "lint:scripts": "eslint src",
    "format": "yarn format:files && yarn fix:styles && yarn fix:scripts && echo Formatting complete successfully",
    "format:files": "prettier --write src",
    "fix:styles": "stylelint --fix src/**/*.{css,scss,sass}",
    "fix:scripts": "eslint --fix src",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "dev": "webpack serve --mode development --config webpack/webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lomazavr/react-starter-kit.git"
  },
  "keywords": [
    "webpack",
    "babel",
    "eslint",
    "prettier",
    "stylelint",
    "starterkit",
    "frontend",
    "es6",
    "esNext",
    "react",
    "javascript",
    "svgr",
    "webdev"
  ],
  "bugs": {
    "url": "https://github.com/lomazavr/react-starter-kit/issues"
  },
  "dependencies": {
    "core-js": "^3.26.0",
    "dotenv": "^16.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.8",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@svgr/webpack": "^6.5.1",
    "babel-loader": "^9.1.0",
    "browserslist": "^4.21.4",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "image-minimizer-webpack-plugin": "^3.8.0",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.18",
    "postcss-loader": "^7.0.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.8.2",
    "postcss-scss": "^4.0.5",
    "postcss-sorting": "^7.0.1",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "react-refresh": "^0.14.0",
    "sass": "^1.56.0",
    "sass-loader": "^13.1.0",
    "serve": "^14.0.1",
    "style-loader": "^3.3.1",
    "stylelint": "^14.14.1",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.6.0",
    "stylelint-order": "^5.0.0",
    "stylelint-webpack-plugin": "^4.0.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "yarn-upgrade-all": "^0.7.1"
  }
}
